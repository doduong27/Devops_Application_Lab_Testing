// Lab 6 Solution: Post Actions
pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Building application...'
            }
        }
    }
    
    post {
        always {
            echo 'Pipeline execution completed'
        }
        
        success {
            echo 'Build succeeded!'
        }
        
        failure {
            echo 'Build failed!'
        }
    }
}




